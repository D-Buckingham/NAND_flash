
// #include <zephyr/dt-bindings/input/input-event-codes.h>


// &qspi{//additionally disabled in proj.conf
//     status = "disabled";
// };

&pinctrl {
    spi4m_default: spi4m_default {
        group1 {
            psels = <NRF_PSEL(SPIM_SCK, 1, 8)>,
                <NRF_PSEL(SPIM_MISO, 1, 10)>,
                <NRF_PSEL(SPIM_MOSI, 1, 9)>;
        };
    };

    spi4m_sleep: spi4m_sleep {
        group1 {
            psels = <NRF_PSEL(SPIM_SCK, 1, 8)>,
                <NRF_PSEL(SPIM_MISO, 1, 9)>,
                <NRF_PSEL(SPIM_MOSI, 1, 10)>;
            low-power-enable;
        };
    };
};






&spi4 {
	compatible = "nordic,nrf-spim";
	status = "okay";
	cs-gpios = <&gpio0 11 GPIO_ACTIVE_LOW>; /* D10 */
	pinctrl-0 = <&spi4m_default>;
	pinctrl-1 = <&spi4m_sleep>;
	pinctrl-names = "default", "sleep";
    reg_my_spi_master: reg_my_spi_master@0 {
        status = "okay";
        compatible = "vnd,spi-device";
        reg = <0>;
        spi-max-frequency = <1000000>;
        label = "reg_my_spi_master";
    };
};